{
  "manifest_version": 2,

  "name":"New Brunswick Office of Innovation",
  "description": "Small description for kiosk goes here",
  "version":"0.1",
  "manifest_version":2,
  "minimum_chrome_version":"37.0.0.0",

  "version": "0.0.0",
  "icons": {
    "128": "chrome-icon-lg.png"
  },
  "permissions": [
    "webview",
      "power",
      "storage",
      "videoCapture",
      "geolocation",
      "pointerLock",
      "system.display",
      {  
         "fileSystem":[  
            "write",
            "retainEntries",
            "directory"
         ]
      },
      "accessibilityFeatures.read",
      "accessibilityFeatures.modify"
  ],

  "chrome_url_overrides": {
    "newtab": "index.html"
  },
   "kiosk_enabled": true,
   "kiosk_only": false
}